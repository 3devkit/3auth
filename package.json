{
  "name": "3auth",
  "description": "web3 wallet login",
  "version": "0.0.0",
  "private": true,
  "author": "WEB3.WALLE <web3.walle@gmail.com>",
  "license": "MIT",
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "repository": {
    "type": "github",
    "url": "https://github.com/3devkit/3auth"
  },
  "scripts": {
    "ex-dev": "cd packages/example && yarn dev",
    "ex-build": "cd packages/example && yarn build",
    "ex-start": "cd packages/example && yarn start",
    "test": "jest",
    "testw": "yarn test --watch",
    "test2": "yarn node --experimental-vm-modules $(yarn bin jest)",
    "test-cov": "yarn test --coverage",
    "start": "cd example && yarn start",
    "dev": "node scripts/build.js --watch --env dev",
    "prod": "node scripts/build.js --env prod",
    "build": "yarn prod",
    "ci-build": "node scripts/ci-build.js",
    "fix": "yarn fix:prettier && yarn fix:eslint",
    "fix:eslint": "yarn lint:eslint --fix",
    "fix:prettier": "yarn lint:prettier --write",
    "lint": "yarn lint:eslint",
    "lint:eslint": "eslint \"./packages/**/src/**\" --ext .ts,.tsx -c ./configs/eslintrc.js --ignore-path ./configs/.eslintignore",
    "lint:prettier": "prettier --list-different \"./packages/**/src/*.{css,js,jsx,ts,tsx}\"",
    "commit": "git add . && git-cz",
    "push": "git push",
    "pull": "git pull --reb",
    "up": "yarn upgrade-interactive --latest",
    "ps-github": "node scripts/publish.js",
    "ps-npm": "lerna publish --no-private",
    "npm:publish": "node scripts/npm-publish.js",
    "clean": "node scripts/dist-clean.js",
    "i": "lerna bootstrap & export NODE_OPTIONS='--max_old_space_size=4096' & yarn build",
    "link-all": "yarn unlink-all && yarn lerna exec --parallel yarn link",
    "unlink-all": "yarn lerna exec --parallel --bail=false yarn unlink",
    "lb": "yarn lerna bootstrap"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.10",
    "@babel/plugin-external-helpers": "^7.18.6",
    "@babel/plugin-transform-modules-commonjs": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime-corejs3": "^7.18.9",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@jest/core": "^28.1.3",
    "@mui/base": "^5.0.0-alpha.92",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-strip": "^2.1.0",
    "@rollup/plugin-typescript": "^8.3.4",
    "@rollup/plugin-url": "^7.0.0",
    "@rollup/pluginutils": "^4.2.1",
    "@svgr/rollup": "6.2.1",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.242",
    "@types/jest": "^28.1.6",
    "@types/less": "^3.0.3",
    "@types/lodash": "^4.14.182",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "babel-jest": "^28.1.3",
    "babel-loader": "^8.2.5",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-imports": "^2.0.0",
    "bootstrap": "^5.2.0",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.9.1",
    "esbuild": "^0.15.5",
    "esbuild-plugin-d.ts": "^1.1.0",
    "eslint": "^8.21.0",
    "eslint-config-alloy": "^4.6.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "hox": "^1.1.6",
    "husky": "^8.0.1",
    "jest": "^28.1.2",
    "jest-watch-yarn-workspaces": "^1.1.0",
    "lerna": "^5.3.0",
    "less": "^4.1.3",
    "lint-staged": "^13.0.3",
    "minimist": "^1.2.6",
    "next": "^12.2.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.13",
    "prettier": "^2.7.1",
    "re-resizable": "^6.9.9",
    "react": "^18.2.0",
    "react-bootstrap": "^2.5.0",
    "react-cool-portal": "^1.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.3.1",
    "react-loading": "^2.0.3",
    "rollup": "^2.77.2",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-esbuild": "^4.10.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-swc3": "^0.3.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.32.1",
    "serve": "^14.0.1",
    "shelljs": "^0.8.5",
    "styled-components": "^5.3.5",
    "ts-jest": "^28.0.7",
    "typescript": "^4.8.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint --config node_modules/@solidoc/iffe-cli/configs/commitlint.js --env HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "yarn test"
    }
  },
  "lint-staged": {
    "./packages/**/src/**": "yarn fix"
  },
  "prettier": "./configs/prettierrc.js",
  "release": {
    "extends": "./configs/releaserc.json"
  },
  "dependencies": {
    "@types/bootstrap": "^5.2.1",
    "@types/react-headroom": "^2.2.2",
    "ahooks": "^3.7.0",
    "axios": "^0.27.2",
    "hooks": "^0.3.2",
    "is-in-browser": "^2.0.0",
    "react-headroom": "^3.2.0",
    "react-jwt": "^1.1.6",
    "react-toastify": "^9.0.5",
    "sass": "^1.54.8",
    "swr": "^1.3.0",
    "zustand": "^4.0.0-rc.3"
  }
}
